# AWS Configuration
# Copy this file to .env and fill in your AWS credentials

# AWS Region (e.g., us-east-1, eu-west-1, ap-southeast-1)
VITE_AWS_REGION=us-east-1

# OPTION 1: Two-User Setup (Recommended - More Secure)
# Read-Only Credentials (for public pages - can only read data)
VITE_AWS_READONLY_ACCESS_KEY_ID=your-readonly-access-key-id-here
VITE_AWS_READONLY_SECRET_ACCESS_KEY=your-readonly-secret-access-key-here

# Read-Write Credentials (for admin pages - can create/update/delete)
VITE_AWS_WRITE_ACCESS_KEY_ID=your-write-access-key-id-here
VITE_AWS_WRITE_SECRET_ACCESS_KEY=your-write-secret-access-key-here

# OPTION 2: Single User Setup (Fallback - Less Secure)
# If two-user setup is not used, these will be used as fallback
VITE_AWS_ACCESS_KEY_ID=your-access-key-id-here
VITE_AWS_SECRET_ACCESS_KEY=your-secret-access-key-here

# S3 Bucket Name for images
VITE_S3_BUCKET_NAME=football-tournaments-images
